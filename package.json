{
  "name": "@advcomm/tenant_replication_postgres",
  "version": "1.0.0",
  "description": "Multi-tenant database replication library with MTDD routing and gRPC support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "node --env-file=.env dist/index.js",
    "dev": "env-cmd -f .env nodemon src/index.ts",
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test:local": "npm run build && npm pack"
  },
  "keywords": [
    "multi-tenant",
    "database",
    "replication", 
    "grpc",
    "knex",
    "postgres",
    "tenant-routing"
  ],
  "author": "dev1@hostingcontroller.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/advcomm/tenant_replication_postgres.git"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@advcomm/utils": "^1.10.0",
    "@grpc/grpc-js": "^1.13.4",
    "express": "^4.21.2",
    "firebase-admin": "^13.4.0",
    "knex": "^3.1.0",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.13.9",
    "env-cmd": "^11.0.0",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2"
  }
}
